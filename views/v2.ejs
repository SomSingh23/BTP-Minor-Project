<%-include('partials/header')%>
    <link rel="stylesheet" href="/css/v1.css" />
    <title>v2</title>
    <script>
      window.onpageshow = function(event) {
              if (event.persisted) {
                  window.location.reload();
              }
          };
    </script>
  </head>
  <body>
    <%-include('partials/navbar')%>
    <h1>Pending Students V2 verification list</h1>
    <ol>
      <% for( let x of data ) { %> <%if(!x.verification2 &&
      x.verification2Status===process.env.STATUS && x.verification1===true) { %>
      <li><a href="/v2/<%=x.googleId%>"><%=x.username%></a></li>
      <%}%> <% } %>
    </ol>
    <%-include('partials/footer')%>
  </body>
</html>
