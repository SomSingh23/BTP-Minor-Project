<%-include('partials/header')%>
    <title>
       Form Already Filled
    </title>
    <style>
      body {
        font-family: Arial, sans-serif;
        background-color: #f0f0f0;
        text-align: center;
      }

      .modal {
        display: none;
        position: fixed;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
        background-color: rgba(0, 0, 0, 0.5);
      }

      .modal-content {
        background-color: #fff;
        border-radius: 5px;
        box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);
        width: 300px;
        margin: 200px auto;
        padding: 20px;
      }

      h1 {
        font-size: 24px;
        margin-bottom: 20px;
      }

      p {
        font-size: 18px;
        margin-bottom: 20px;
      }

      footer{
        margin-top: 600px;
      }
    </style>
  </head>
  <body>
    <%-include('partials/navbar')%>
    <div class="modal" id="myModal">
      <div class="modal-content">
        <h1>You have Already Filled the Form</h1>
        <p>Kindly Check Status.</p>
       
      </div>
    </div>

    <script>
      
      window.onload = function () {
        var modal = document.getElementById("myModal");
        modal.style.display = "block";
      };
    </script>
    <%-include('partials/footer')%>
  </body>
</html>
